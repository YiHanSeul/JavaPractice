package com.arrayPractice1.sample;

public class ArraySample {

	public void test2() {
		int ar[] = new int[10];

		for (int i = 0; i < ar.length; i++) {
			ar[i] = (int) (Math.random() * 100) + 1;
			System.out.print(ar[i]+" ");
		}
		System.out.println();
		
		int max=ar[0];
		int min=ar[0];
		
		for(int i=0;i<ar.length;i++) {
			if(max<ar[i]) {
				max=ar[i];
			}
			if(min>ar[i]) {
				min=ar[i];
			}
		}
	}
}
