package com.test01;

public class DeArrayTest {
	public void testInit() {

		// 2차원 배열
		int[][] iarr1;
		int iarr2[][];

		// 할당
		iarr1 = new int[3][5];
		iarr2 = new int[3][];
		iarr2[0] = new int[5]; // 1번째방에 5개방을 만듬. 2차원 배열
		iarr2[1] = new int[5];
		iarr2[2] = new int[5];

		// 2중 for문을 이용하여 배열의 값 출력
		for (int i = 0; i < 3; i++) {
			for (int j = 0; j < 5; j++) {
				System.out.print(iarr2[i][j] + " ");
			}
			System.out.println();
		}
	}

	public void testInit2() {
		int[][] iarr=new int[3][5];
		
		iarr[0][0]=1;
		iarr[0][1]=2;
		iarr[0][2]=3;
		iarr[0][3]=4;
		iarr[0][4]=5;
		
		iarr[1][0]=5;
		iarr[1][1]=6;
		iarr[1][2]=7;
		iarr[1][3]=8;
		iarr[1][4]=9;
		
		iarr[2][0]=10;
		iarr[2][1]=11;
		iarr[2][2]=12;
		iarr[2][3]=13;
		iarr[2][4]=14;
		
		//이중 for문을 이용한 값 기록
		int val=100;
		for() {
			for() {
				
			}
		}
		printArr(iarr);
	}

	public void printArr(int[][] iarr) {
		for (int i = 0; i < iarr.length; i++) {
			for (int j = 0; j < iarr[i].length; j++) {
				System.out.print(iarr[i][j] + " ");
			}
			System.out.println();
		}
	}
}
