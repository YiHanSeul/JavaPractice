package com.test01.model.dto;

public class Book {
	//ÇÊµå
	private String title;
	private String author;
	private String publisher;
	private int price;
	
	public Book() {}
	
	public Book(String title,String author, String publisher, int price) {
		this.title=title;
		this.author=author;
		this.publisher=publisher;
		this.price=price;
	}

}
