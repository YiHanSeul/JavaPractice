package com.test01;

public class DeArrayTest {
	public void testInit() {

		// 2차원 배열
		int[][] iarr1;
		int iarr2[][];

		// 할당
		iarr1 = new int[3][5];
		iarr2 = new int[3][];
		iarr2[0] = new int[5]; // 1번째방에 5개방을 만듬. 2차원 배열
		iarr2[1] = new int[5];
		iarr2[2] = new int[5];

		// 2중 for문을 이용하여 배열의 값 출력
		for (int i = 0; i < 3; i++) {
			for (int j = 0; j < 5; j++) {
				System.out.print(iarr2[i][j]+ " ");
			}
			System.out.println();
		}
	}
}
